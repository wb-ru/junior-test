package main

import "fmt"

func main() {
	var nums []int = []int{1, 2, 3, 3, 0}
	var target int = 3
	var i []int
	i = append(twoSum(nums, target))
	fmt.Print(i)
}

func twoSum(nums []int, target int) []int {
	for i := 0; i < len(nums); i++ {
		for j := i + 1; j < len(nums); j++ {
			if nums[i]+nums[j] == target {
				return []int{i, j}
			}
		}
	}
	return []int{0}
}
